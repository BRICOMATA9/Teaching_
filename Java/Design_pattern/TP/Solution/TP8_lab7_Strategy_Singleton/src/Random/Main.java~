package Random;

import java.util.List;
import java.util.ArrayList;
import java.util.Arrays;

public class Main {
	
	public static void main(String[] args) {
		List<String> list = new ArrayList<String>(Arrays.asList("Aghiles","Djoudi"));
		RandomSequence random = new Congruential(3);
		for (String s:list) System.out.println(s+" is seat "+random.geNext());
	}
}

public class point
{
 private double centre;
 public point (double x, double y)
 {
  this.centre=(x+y)/2;
 }
 
 public point ()
 {
  this.centre=0;
 }

 public double getcentre()
 {return centre;
 }
 public String toString()
 {return ("("+x+","+y+")");
 }
 public void affichepoint()
 {System.put.println(toString());
 }
}


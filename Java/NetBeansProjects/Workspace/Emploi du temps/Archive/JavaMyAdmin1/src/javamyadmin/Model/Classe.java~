package Model;

public class Classe extends NamedEntity {
	private String idPromotion;
	private EmploiDuTemps emploi;

	public EmploiDuTemps getEmploi() {
		return emploi;
	}

	public void setEmploi(EmploiDuTemps emploi) {
		this.emploi = emploi;
	}

	public String getIdPromotion() {
		return idPromotion;
	}

	public void setIdPromotion(String idPromotion) {
		this.idPromotion = idPromotion;
	}

}

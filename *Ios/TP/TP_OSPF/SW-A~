int fa 0/5
sw mode access
sw access vlan 2

int fa 0/10
sw mode access
sw access vlan 4

int fa 0/15
sw mode access
sw access vlan 8

int fa 0/24
sw mode access
sw access vlan 15
exit

int range fa0/6-9, fa0/11-14, fa0/16-23, g0/1-2
sw mode access
sw access vlan 99
sh
exit

int vlan 25
ip add 10.10.25.254 255.255.255.0
no sh
exit

ip default-gat 10.10.25.1

int range fa0/1-2
sw mode trunk
channel-group 1 mode active
exit

int port-channel 1
sw mode trunk

int range fa0/3-4
sw mode trunk
channel-group 2 mode active
exit

int port-channel 2
sw mode trunk
exit

spanning-tree mode rapid
spanning-tree vlan 2,4 root primary
spanning-tree vlan 8,15 root secondary

int range fa0/5, fa0/10, fa0/15, fa0/24
sw port-security 
sw port-security max 2
